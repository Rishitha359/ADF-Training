{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfMiniProject001"
		},
		"LS_Postgres_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Postgres'"
		},
		"LS_Sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Sql'"
		},
		"LS_Postgres_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"LS_Postgres_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Event_Planner"
		},
		"LS_Postgres_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"LS_Sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbs-training001.database.windows.net"
		},
		"LS_Sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_training001"
		},
		"LS_Sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rishitha"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfMiniProject001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Sql_password": {
						"value": "[parameters('LS_Sql_password')]"
					},
					"LS_Postgres_password": {
						"value": "[parameters('LS_Postgres_password')]"
					},
					"LS_Sql_properties_typeProperties_server": {
						"value": "[parameters('LS_Sql_properties_typeProperties_server')]"
					},
					"LS_Sql_properties_typeProperties_database": {
						"value": "[parameters('LS_Sql_properties_typeProperties_database')]"
					},
					"LS_Sql_properties_typeProperties_userName": {
						"value": "[parameters('LS_Sql_properties_typeProperties_userName')]"
					},
					"LS_Postgres_properties_typeProperties_server": {
						"value": "[parameters('LS_Postgres_properties_typeProperties_server')]"
					},
					"LS_Postgres_properties_typeProperties_database": {
						"value": "[parameters('LS_Postgres_properties_typeProperties_database')]"
					},
					"LS_Postgres_properties_typeProperties_username": {
						"value": "[parameters('LS_Postgres_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}